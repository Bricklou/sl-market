{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "include": [
    "app",
    "commands",
    "providers",
    "start",
    "models"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "report-dir": "coverage",
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
