{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "exclude": [
    "node_modules",
    "resources/app",
    "public",
    "*.test.*",
    "coverage"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "report-dir": "coverage",
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
